set nocompatible
set showcmd

syntax on
filetype on

set autoindent

set smartindent
set expandtab
set shiftwidth=4
set softtabstop=4

if version >= 700
    set spl=en spell
    set nospell
endif

set wildmenu
"set wildmode=list:longest,full

set mouse=a

set number

set ignorecase

set smartcase

set incsearch

set nohidden

set hlsearch

let g:clipbrdDefaultReg = '+'

set nohidden

set nowrap

set clipboard=unnamed
